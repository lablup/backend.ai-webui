{
  "$schema": "./theme.schema.json",
  "light" : {
    "token": {
      "fontFamily": "'Ubuntu', Roboto, sans-serif",
      "colorPrimary": "#FF7A00",
      "colorLink": "#FF7A00",
      "colorText": "#141414",
      "colorInfo": "#028DF2",
      "colorError": "#FF4D4F"
    },
    "components": {
      "Tag": {
        "borderRadiusSM": 1
      },
      "Table": {
        "headerBorderRadius": 0
      },
      "Layout": {
        "lightSiderBg": "#FFF",
        "siderBg": "#2A2C30",
        "headerBg": "#FF9729"
      }
    }
  },
  "dark": {
    "token": {
      "fontFamily": "'Ubuntu', Roboto, sans-serif",
      "colorPrimary": "#DC6B03",
      "colorText": "#FFF",
      "colorInfo": "#009BDD",
      "colorError": "#DC4446"
    },
    "components": {
      "Tag": {
        "borderRadiusSM": 1
      },
      "Table": {
        "headerBorderRadius": 0
      },
      "Layout": {
        "lightSiderBg": "#F4F7FD",
        "siderBg": "#2A2C30",
        "headerBg": "#E88A28"
      }
    }
  },
  "logo": {
    "src": "/manifest/backend.ai-webui-white.svg",
    "srcCollapsed": "/manifest/backend.ai-brand-simple-white.svg",
    "srcDark": "/manifest/backend.ai-webui-white.svg",
    "srcCollapsedDark": "/manifest/backend.ai-brand-simple-white.svg",
    "logoTitle": "WebUI",
    "logoTitleCollapsed": "WebUI",
    "alt": "Backend.AI Logo",
    "href": "/summary",
    "sizeCollapsed": {
      "width": 34,
      "height": 34
    }
  },
  "sider": {
  },
  "branding": {
    "companyName": "Lablup Inc.",
    "brandName": "Backend.AI"
  }
}
