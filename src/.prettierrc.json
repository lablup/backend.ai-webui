{
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "importOrderSeparation": true,
  "importOrderParserPlugins": ["typescript"],
  "printWidth": 80,
  "bracketSpacing": true,
  "trailingComma": "all",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "htmlWhitespaceSensitivity": "ignore",
  "singleAttributePerLine": false
}
