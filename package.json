    "chartjs-adapter-date-fns": "^2.0.0",
    "crypto-browserify": "^3.12.0",
    "crypto-es": "^1.2.7",
    "date-fns": "^2.28.0",
    "drawflow": "^0.0.55",
    "https-proxy-agent": "^5.0.1",
    "js-yaml": "^4.1.0",
