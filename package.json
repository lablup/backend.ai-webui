    "babel-eslint": "^10.1.0",
    "browserify": "^17.0.0",
    "buffer": "^6.0.3",
    "cors": "^2.8.5",
    "electron": "^14.2.4",
    "electron-installer-debian": "^3.1.0",
    "electron-installer-dmg": "^4.0.0",
    "electron-packager": "^15.5.1",
    "es-dev-server": "^2.1.0",
    "eslint": "^8.14.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-lit-a11y": "^2.2.0",
    "eslint-plugin-wc": "^1.3.2",
    "express": "^4.18.1",
    "i18next-scanner": "^3.1.0",
    "jsdoc": "^3.6.10",
    "node-fetch": "^3.2.6",
