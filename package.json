    "rollup": "^2.18.1",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript": "^1.0.1",
    "serve": "^11.3.2",
    "testcafe": "^1.8.7",
    "tinyify": "^2.5.2",
    "tslib": "^2.0.0",
    "typescript": "^3.9.5",
    "underscore": "^1.10.2",
    "web-animations-js": "^2.3.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "workbox-expiration": "^5.1.3",
    "workbox-routing": "^5.1.3",
    "workbox-strategies": "^5.1.3",
