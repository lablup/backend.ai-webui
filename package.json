    "@material/mwc-list": "^0.20.0",
    "@material/mwc-menu": "^0.20.0",
    "@material/mwc-select": "^0.20.0",
    "@material/mwc-slider": "^0.20.0",
    "@material/mwc-switch": "^0.20.0",
    "@material/mwc-tab-bar": "^0.20.0",
    "@material/mwc-textarea": "^0.20.0",
    "@material/mwc-textfield": "^0.20.0",
    "@material/mwc-top-app-bar-fixed": "^0.20.0",
    "@vaadin/vaadin-grid": "^6.0.1",
    "@vaadin/vaadin-icons": "^4.3.2",
    "@vaadin/vaadin-item": "^3.0.0",
    "@vaadin/vaadin-progress-bar": "^1.3.0",
    "@vanillawc/wc-codemirror": "^1.9.1",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "bufferutil": "^4.0.3",
    "chart.js": "^3.0.0-alpha.2",
    "chartjs-adapter-date-fns": "^1.1.0-alpha",
    "date-fns": "^2.19.0",
    "https-proxy-agent": "^5.0.0",
    "lit-element": "^2.4.0",
    "lit-html": "^1.3.0",
    "lit-translate": "^1.2.1",
    "log": "^6.0.0",
    "macro-carousel": "^1.0.0",
    "marked": "^2.0.1",
    "markty-toml": "0.1.1",
    "tus-js-client": "^2.2.0",
    "utf-8-validate": "^5.0.4",
    "weightless": "^0.0.37",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/parser": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/types": "^7.13.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^2.4.1",
    "@rollup/plugin-typescript": "^8.2.0",
    "@types/estree": "0.0.46",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "babel-eslint": "^10.1.0",
    "better-docs": "^2.3.2",
    "browserify": "^17.0.0",
    "buffer": "^6.0.3",
    "cors": "^2.8.5",
    "electron": "^12.0.0",
    "electron-installer-debian": "^3.1.0",
    "electron-installer-dmg": "^3.0.0",
    "electron-packager": "^15.2.0",
    "electron-tabs": "^0.15.0",
    "es-dev-server": "^2.1.0",
    "eslint": "^7.21.0",
    "eslint-config-google": "^0.14.0",
    "express": "^4.17.1",
    "i18next-scanner": "^3.0.0",
    "jsdoc": "^3.6.6",
    "node-fetch": "^2.6.1",
    "pwa-helpers": "^0.9.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "rollup": "^2.41.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-workbox": "^6.1.1",
    "serve": "^11.3.2",
    "testcafe": "^1.12.0",
    "tinyify": "^3.0.0",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3",
    "underscore": "^1.12.0",
    "web-animations-js": "^2.3.2",
    "webpack": "^5.24.4",
    "webpack-cli": "^4.5.0",
    "workbox-expiration": "^6.1.1",
    "workbox-routing": "^6.1.1",
    "workbox-strategies": "^6.1.1",
    "workbox-sw": "^6.1.1",
    "ws": "^7.4.4"
  },
  "vaadin": {
    "disableUsageStatistics": true
  }
}
