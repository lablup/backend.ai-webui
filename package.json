{
  "name": "backend.ai-console",
  "version": "20.02.0",
  "repository": "https://github.com/lablup/backend.ai-console.git",
  "author": "Lablup Inc. <contact@lablup.com>",
  "license": "LGPL-3.0-or-later",
  "main": "main.js",
  "scripts": {
    "lint": "eslint *.js",
    "test": "node ./src/wsproxy/local_proxy.js",
    "wsproxy": "node ./src/wsproxy/local_proxy.js",
    "build": "rm -rf build/rollup && mkdir -p build/rollup/src/components && npm run copyindex && npm run copywc && npm run copywa && npm run copyresource && npm run copyconfig && rollup -c rollup.config.ts",
    "server:p": "./node_modules/serve/bin/serve.js build/rollup",
    "server:d": "./node_modules/es-dev-server/dist/cli.js --file-extensions .ts --app-index index.html --watch --node-resolve --port 9081 --compatibility min",
    "build:d": "./node_modules/typescript/bin/tsc --watch",
    "electron:d": "./node_modules/electron/cli.js .",
    "copywc": "mkdir -p build/rollup/src/lib && cp -r node_modules/@webcomponents/webcomponentsjs/bundles build/rollup/src/lib && cp -r node_modules/@webcomponents/webcomponentsjs/bundles src/lib && cp node_modules/@webcomponents/webcomponentsjs/webcomponents-loader.js build/rollup/src/lib && cp -r node_modules/@webcomponents/webcomponentsjs/webcomponents-loader.js src/lib",
    "copywa": "mkdir -p build/rollup/src/lib/web-animations-js && mkdir -p src/lib/web-animations-js && cp node_modules/web-animations-js/web-animations-next-lite.min.js src/lib/web-animations-js && cp src/lib/web-animations-js/web-animations-next-lite.min.js build/rollup/src/lib/web-animations-js",
    "copyresource": "cp -Rp resources build/rollup && cp -Rp manifest build/rollup",
    "copyconfig": "cp config.toml build/rollup && cp version.json build/rollup && cp manifest.json build/rollup",
    "copyindex": "cp index.html build/rollup",
    "update": "./node_modules/typescript/bin/tsc"
  },
  "dependencies": {
    "@material/mwc-drawer": "^0.12.0",
    "@material/mwc-icon-button": "^0.12.0",
    "@material/mwc-slider": "^0.12.0",
    "@material/mwc-top-app-bar-fixed": "^0.12.0",
    "@polymer/iron-dropdown": "^3.0.1",
    "@polymer/iron-icon": "^3.0.1",
    "@polymer/iron-icons": "^3.0.1",
    "@polymer/iron-image": "^3.0.2",
    "@polymer/paper-button": "^3.0.1",
    "@polymer/paper-checkbox": "^3.1.0",
    "@polymer/paper-dialog": "^3.0.1",
    "@polymer/paper-dialog-scrollable": "^3.0.1",
    "@polymer/paper-dropdown-menu": "^3.1.0",
    "@polymer/paper-icon-button": "^3.0.2",
    "@polymer/paper-input": "^3.2.0",
    "@polymer/paper-item": "^3.0.1",
    "@polymer/paper-listbox": "^3.0.1",
    "@polymer/paper-material": "^3.0.1",
    "@polymer/paper-progress": "^3.0.1",
    "@polymer/paper-slider": "^3.0.1",
    "@polymer/paper-styles": "^3.0.1",
    "@polymer/polymer": "^3.3.1",
    "@vaadin/vaadin-dialog": "^2.2.1",
    "@vaadin/vaadin-grid": "^5.5.0",
    "@vaadin/vaadin-icons": "^4.3.1",
    "@vaadin/vaadin-item": "^2.1.1",
    "@vaadin/vaadin-progress-bar": "^1.1.2",
    "@vaadin/vaadin-upload": "^4.2.2",
    "@vanillawc/wc-codemirror": "^1.4.1",
    "@webcomponents/webcomponentsjs": "^2.4.1",
    "d3": "^5.15.0",
    "lit-element": "^2.2.1",
    "lit-html": "^1.1.2",
    "log": "^6.0.0",
    "markty-toml": "0.1.1",
    "weightless": "^0.0.37"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "browserify": "^16.5.0",
    "buffer": "^5.4.3",
    "cors": "^2.8.5",
    "electron": "^7.1.11",
    "electron-installer-dmg": "^3.0.0",
    "electron-packager": "^14.2.0",
    "electron-tabs": "^0.11.0",
    "es-dev-server": "^1.36.4",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "express": "^4.17.1",
    "node-fetch": "^2.6.0",
    "pwa-helpers": "^0.9.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "rollup": "^1.30.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-typescript": "^1.0.1",
    "serve": "^11.3.0",
    "testcafe": "^1.8.1",
    "tinyify": "^2.5.2",
    "tslib": "^1.10.0",
    "typescript": "^3.7.5",
    "underscore": "^1.9.2",
    "web-animations-js": "^2.3.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "ws": "^7.2.1"
  },
  "vaadin": {
    "disableUsageStatistics": true
  }
}
