{
  "name": "backend.ai-webui",
  "productName": "Backend.AI Desktop",
  "version": "26.2.0-alpha.0",
  "repository": "https://github.com/lablup/backend.ai-webui.git",
  "author": "Lablup Inc. <contact@lablup.com>",
  "license": "LGPL-3.0-or-later",
  "engines": {
    "pnpm": "^10.16.0"
  },
  "scripts": {
    "lint": "eslint src --quiet; exit 0",
    "lint-fix": "eslint src --quiet --fix; exit 0",
    "format": "prettier --check 'src/**/*.{js, ts}'",
    "format-fix": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "format-fix:all": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}' 'react/src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "format-fix:i18n": "prettier --write 'resources/i18n/*.json'",
    "test": "jest",
    "wsproxy": "node ./src/wsproxy/local_proxy.js",
    "build": "rm -rf build/rollup && mkdir -p build/rollup && pnpm run copyindex && pnpm run copyresource && pnpm run copyconfig && rollup -c rollup.config.ts --configPlugin typescript && pnpm run -r --stream build",
    "build:react-only": "pnpm run --prefix ./react build:only",
    "server:p": "serve build/rollup",
    "server:d": "eval \"$(node scripts/dev-config.js env)\" && node scripts/dev-config.js update && PORT=$BAI_WEBUI_DEV_REACT_PORT PROXY=http://localhost:$BAI_WEBUI_DEV_WEBDEV_PORT pnpm --prefix ./react run start",
    "build:d": "eval \"$(node scripts/dev-config.js env)\" && node scripts/dev-config.js update && concurrently -c \"auto\" --names \"react-relay,react\" \"cd react && pnpm run relay:watch\" \"BAI_WEBUI_DEV_PROXY= PORT=$BAI_WEBUI_DEV_REACT_PORT pnpm --prefix ./react run start\"",
    "electron:d": "electron build/electron-app --dev",
    "electron:d:hmr": "LIVE_DEBUG=1 electron build/electron-app --dev",
    "relay": "relay-compiler",
    "copyresource": "cp -Rp resources build/rollup && cp -Rp manifest build/rollup",
    "copyconfig": "./scripts/copy-config.js && cp version.json build/rollup && cp manifest.json build/rollup",
    "copyindex": "cp index.html build/rollup",
    "update": "pnpm dlx tsc",
    "prepare": "husky",
    "theme-schema:update": "pnpm dlx typescript-json-schema \"./react/node_modules/antd/es/config-provider/context.d.ts\" ThemeConfig -o ./resources/antdThemeConfig.schema.json --esModuleInterop",
    "dev:config": "node scripts/dev-config.js show",
    "dev:setup": "node scripts/dev-config.js update"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "bufferutil": "^4.1.0",
    "chart.js": "~4.4.9",
    "chartjs-adapter-date-fns": "^3.0.0",
    "commander": "^12.1.0",
    "date-fns": "^3.6.0",
    "dompurify": "catalog:",
    "dotenv": "^16.6.1",
    "https-proxy-agent": "^7.0.6",
    "log": "^6.3.2",
    "marked": "catalog:",
    "markty": "^0.0.4",
    "markty-toml": "catalog:",
    "mime-db": "^1.54.0",
    "mime-types": "^2.1.35",
    "nanoid": "^5.1.6",
    "octokit": "^5.0.5",
    "tus-js-client": "catalog:",
    "utf-8-validate": "^6.0.6",
    "uuid": "catalog:",
    "vis-data": "^7.1.10",
    "vis-network": "^9.1.13",
    "vis-util": "^5.0.7",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@babel/core": "catalog:",
    "@babel/parser": "^7.28.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.28.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-typescript": "catalog:",
    "@babel/types": "^7.28.6",
    "@electron/packager": "^18.4.4",
    "@eslint/js": "catalog:",
    "@jest/expect": "catalog:",
    "@jest/globals": "catalog:",
    "@playwright/test": "^1.58.0",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@rollup/plugin-replace": "^5.0.7",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@trivago/prettier-plugin-sort-imports": "catalog:",
    "@types/estree": "1.0.5",
    "@types/hammerjs": "^2.0.46",
    "@types/jest": "catalog:",
    "@types/lodash": "catalog:",
    "@types/node": "catalog:",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@yao-pkg/pkg": "^6.12.0",
    "babel-eslint": "^10.1.0",
    "buffer": "^6.0.3",
    "cli-color": "^2.0.4",
    "concurrently": "^8.2.2",
    "cors": "^2.8.6",
    "crypto-es": "^2.1.0",
    "electron": "^35.7.5",
    "eslint": "catalog:",
    "eslint-config-bai": "workspace:*",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-json-schema-validator": "catalog:",
    "eslint-plugin-prettier": "^5.5.5",
    "express": "^4.22.1",
    "glob": "^10.5.0",
    "globals": "catalog:",
    "husky": "^9.1.7",
    "i18next-scanner": "^4.6.0",
    "jest": "catalog:",
    "jsonc-eslint-parser": "catalog:",
    "lint-staged": "^15.5.2",
    "lodash": "catalog:",
    "node-fetch": "^3.3.2",
    "prettier": "catalog:",
    "prettier-plugin-sort-json": "catalog:",
    "relay-compiler": "catalog:",
    "rollup": "^4.56.0",
    "rollup-plugin-workbox": "^8.1.2",
    "serve": "^14.2.5",
    "ts-jest": "catalog:",
    "tslib": "^2.8.1",
    "typescript": "~5.5.4",
    "underscore": "^1.13.7",
    "vis-util": "^5.0.7",
    "webpack": "catalog:",
    "webpack-cli": "^6.0.1",
    "workbox-expiration": "^7.4.0",
    "workbox-routing": "^7.4.0",
    "workbox-strategies": "^7.4.0",
    "workbox-sw": "^7.4.0",
    "ws": "^8.19.0"
  },
  "pnpm": {
    "overrides": {
      "tar-fs@2": "2.1.4",
      "node-forge": ">=1.3.2"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ],
    "react/src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "pnpm run --prefix ./react lint --cache",
      "prettier --write"
    ],
    "packages/backend.ai-ui/src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "pnpm run --prefix ./packages/backend.ai-ui lint --cache",
      "prettier --write"
    ],
    "e2e/**/*.{js,ts}": [
      "eslint --config e2e/eslint.config.mjs --max-warnings=0 --cache",
      "prettier --write"
    ],
    "resources/i18n/*.json": [
      "eslint --max-warnings=0 --cache",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "rootDir": "./",
    "roots": [
      "scripts",
      "src"
    ]
  }
}
