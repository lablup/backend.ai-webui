  "scripts": {
    "start": "craco start",
    "build": "pnpm run build:only && cp -r ./build/* ../build/rollup/",
    "build:only": "pnpm run relay && craco build",
    "test": "NODE_OPTIONS='$NODE_OPTIONS --no-deprecation --experimental-vm-modules' jest",
    "eject": "react-scripts eject",
    "relay": "relay-compiler",
    "relay:watch": "nodemon --watch schema.graphql --watch client-directives.graphql --exec 'pnpm run relay --watch'",
    "lint": "eslint ./src '../resources/i18n/*.json' '../resources/device_metadata.json' --max-warnings=0",
    "format": "prettier --check 'src/**/*.{js,jsx,ts,tsx}'",
    "format-fix": "prettier --write './src/**/*.{js,jsx,ts,tsx}'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended",
      "plugin:react/recommended",
      "plugin:relay/recommended",
      "plugin:react-hooks/recommended"
    ],
    "plugins": [
      "relay",
      "import"
    ],
    "overrides": [
      {
        "files": [
          "**/*.test.*",
          "**/*.stories.*"
        ],
        "rules": {
          "no-console": "off"
        }
      }
    ],
    "rules": {
      "new-cap": "off",
      "prefer-promise-reject-errors": "off",
      "react/react-in-jsx-scope": "off",
      "react/prop-types": "off",
      "import/no-duplicates": "error",
      "relay/graphql-syntax": "off",
      "relay/unused-fields": "off",
      "relay/must-colocate-fragment-spreads": "off",
      "comma-spacing": "off",
      "@typescript-eslint/no-unused-vars": [
        "warn",
        {
          "args": "all",
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_"
        }
      ],
      "no-restricted-imports": [
        "error",
        {
          "patterns": [
            "backend.ai-ui/*",
            "!backend.ai-ui/dist",
            "@lobehub/fluent-emoji"
          ],
          "paths": [
            {
              "name": "antd-style",
              "importNames": ["useThemeMode"],
              "message": "Use 'src/hooks/useThemeMode' instead of 'antd-style' for useThemeMode."
            }
          ]
        }
      ],
      "no-console": [
        "warn"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
