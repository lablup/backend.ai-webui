{
  "name": "backend.ai-ui",
  "version": "26.2.0-alpha.0",
  "description": "React components for Backend.AI",
  "repository": {
    "type": "git",
    "url": "https://github.com/lablup/backend.ai-webui/tree/main/packages/backend.ai-ui"
  },
  "bugs": {
    "url": "https://github.com/lablup/backend.ai-webui/issues"
  },
  "author": "Lablup Inc. <contact@lablup.com>",
  "license": "LGPL-3.0-or-later",
  "keywords": [
    "backend.ai",
    "component",
    "react"
  ],
  "main": "./dist/backend.ai-ui.js",
  "module": "./dist/backend.ai-ui.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/backend.ai-ui.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/locale/*": {
      "import": "./dist/locale/*.js",
      "types": "./dist/locale/*.d.ts"
    }
  },
  "files": [
    "dist",
    "dist/locale"
  ],
  "scripts": {
    "dev": "vite build --watch --mode development",
    "build": "vite build",
    "test": "NODE_OPTIONS='--no-deprecation' jest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint ./src --ignore-pattern '**.graphql.**' --max-warnings=0",
    "prepublishOnly": "pnpm lint && pnpm test && pnpm build"
  },
  "peerDependencies": {
    "@ant-design/icons": "^6.1.0",
    "@tanstack/react-query": "^5.90.19",
    "ahooks": "^3.9.6",
    "antd": "^6.2.1",
    "antd-style": "^3.7.1",
    "graphql": "^16.12.0",
    "i18next": "^25.8.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-i18next": "^16.5.3",
    "react-relay": "^20.1.1",
    "react-router-dom": "^6.30.3",
    "relay-runtime": "^20.1.1"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "big.js": "^7.0.1",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.19",
    "lodash": "^4.17.23",
    "lucide-react": "^0.552.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-draggable": "^4.5.0",
    "react-resizable": "^3.1.3"
  },
  "devDependencies": {
    "eslint-config-bai": "workspace:*",
    "@jest/expect": "30.2.0",
    "@jest/globals": "30.2.0",
    "@storybook/addon-docs": "^10.1.11",
    "@storybook/addon-onboarding": "^10.1.11",
    "@storybook/react-vite": "^10.1.11",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/big.js": "^6.2.2",
    "@types/lodash": "^4.17.23",
    "@types/react": "^19.2.9",
    "@types/react-copy-to-clipboard": "^5.0.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-relay": "^18.2.1",
    "@types/react-resizable": "^3.0.8",
    "@types/relay-runtime": "^19.0.3",
    "@types/relay-test-utils": "^19.0.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vueless/storybook-dark-mode": "^10.0.6",
    "babel-jest": "^30.2.0",
    "babel-plugin-react-compiler": "^1.0.0",
    "@eslint/js": "^9.18.0",
    "eslint": "^9.18.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-json-schema-validator": "^5.5.1",
    "eslint-plugin-storybook": "^10.1.11",
    "globals": "^16.0.0",
    "jsonc-eslint-parser": "^2.4.2",
    "typescript-eslint": "^8.23.0",
    "fast-glob": "^3.3.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier-plugin-sort-json": "^4.2.0",
    "relay-test-utils": "^20.1.1",
    "storybook": "^10.1.11",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3",
    "vite": "^6.4.1",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-relay-lite": "^0.11.0",
    "vite-plugin-svgr": "^4.5.0"
  },
  "type": "module",
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "extensionsToTreatAsEsm": [
      ".ts",
      ".tsx"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": [
        "ts-jest",
        {
          "useESM": true,
          "tsconfig": {
            "jsx": "react-jsx"
          }
        }
      ],
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "setupFilesAfterEnv": [
      "./setupTests.ts"
    ],
    "testEnvironment": "jsdom"
  }
}
