{
  "name": "backend.ai-ui",
  "version": "1.0.0",
  "private": true,
  "description": "Shared UI components for Backend.AI",
  "main": "./dist/backend.ai-ui.js",
  "module": "./dist/backend.ai-ui.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/backend.ai-ui.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/locale/*": {
      "import": "./dist/locale/*.js",
      "types": "./dist/locale/*.d.ts"
    }
  },
  "files": [
    "dist",
    "dist/locale"
  ],
  "scripts": {
    "dev": "vite build --watch --mode development",
    "build": "vite build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint ./src --ignore-pattern '*.test.*' --ignore-pattern '**.graphql.**' --max-warnings=0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended",
      "plugin:react/recommended"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off",
      "react/prop-types": "off",
      "import/no-duplicates": "error"
    }
  },
  "peerDependencies": {
    "antd": "^5.26.6",
    "graphql": "^16.11.0",
    "lodash": "^4.17.21",
    "i18next": "^25.3.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.6.0",
    "react-relay": "^20.0.0",
    "relay-runtime": "^20.0.0"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^9.0.0",
    "@storybook/addon-essentials": "^8.4.5",
    "@storybook/addon-interactions": "^8.4.5",
    "@storybook/addon-links": "^8.4.5",
    "@storybook/addon-onboarding": "^9.0.0",
    "@storybook/react-vite": "^9.0.0",
    "@types/lodash": "^4.17.13",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-relay": "^18.2.0",
    "@vitejs/plugin-react": "^4.5.0",
    "eslint": "^8.57.1",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-storybook": "^9.0.0",
    "fast-glob": "^3.3.3",
    "graphql": "^16.10.0",
    "storybook": "^9.0.0",
    "typescript": "^5.7.2",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-relay-lite": "^0.11.0"
  },
  "type": "module"
}
