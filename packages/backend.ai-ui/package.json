{
  "name": "backend.ai-ui",
  "version": "26.2.0-alpha.0",
  "description": "React components for Backend.AI",
  "repository": {
    "type": "git",
    "url": "https://github.com/lablup/backend.ai-webui/tree/main/packages/backend.ai-ui"
  },
  "bugs": {
    "url": "https://github.com/lablup/backend.ai-webui/issues"
  },
  "author": "Lablup Inc. <contact@lablup.com>",
  "license": "LGPL-3.0-or-later",
  "keywords": [
    "backend.ai",
    "component",
    "react"
  ],
  "main": "./dist/backend.ai-ui.js",
  "module": "./dist/backend.ai-ui.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/backend.ai-ui.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/locale/*": {
      "import": "./dist/locale/*.js",
      "types": "./dist/locale/*.d.ts"
    }
  },
  "files": [
    "dist",
    "dist/locale"
  ],
  "scripts": {
    "dev": "vite build --watch --mode development",
    "build": "vite build",
    "test": "NODE_OPTIONS='--no-deprecation' jest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint ./src --ignore-pattern '**.graphql.**' --max-warnings=0",
    "prepublishOnly": "pnpm lint && pnpm test && pnpm build"
  },
  "peerDependencies": {
    "@ant-design/icons": "catalog:",
    "@tanstack/react-query": "catalog:",
    "ahooks": "catalog:",
    "antd": "catalog:",
    "antd-style": "catalog:",
    "graphql": "catalog:",
    "i18next": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-i18next": "catalog:",
    "react-relay": "catalog:",
    "react-router-dom": "catalog:",
    "relay-runtime": "catalog:"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "big.js": "catalog:",
    "classnames": "catalog:",
    "dayjs": "catalog:",
    "lodash": "catalog:",
    "lucide-react": "catalog:",
    "react-copy-to-clipboard": "catalog:",
    "react-draggable": "^4.5.0",
    "react-resizable": "^3.1.3"
  },
  "devDependencies": {
    "eslint-config-bai": "workspace:*",
    "@jest/expect": "catalog:",
    "@jest/globals": "catalog:",
    "@storybook/addon-docs": "^10.1.11",
    "@storybook/addon-onboarding": "^10.1.11",
    "@storybook/react-vite": "^10.1.11",
    "@testing-library/jest-dom": "catalog:",
    "@testing-library/react": "catalog:",
    "@testing-library/user-event": "catalog:",
    "@trivago/prettier-plugin-sort-imports": "catalog:",
    "@types/big.js": "catalog:",
    "@types/lodash": "catalog:",
    "@types/react": "catalog:",
    "@types/react-copy-to-clipboard": "catalog:",
    "@types/react-dom": "catalog:",
    "@types/react-relay": "catalog:",
    "@types/react-resizable": "^3.0.8",
    "@types/relay-runtime": "catalog:",
    "@types/relay-test-utils": "catalog:",
    "@vitejs/plugin-react": "^4.7.0",
    "@vueless/storybook-dark-mode": "^10.0.6",
    "babel-jest": "catalog:",
    "babel-plugin-react-compiler": "catalog:",
    "@eslint/js": "catalog:",
    "eslint": "catalog:",
    "eslint-plugin-import": "catalog:",
    "eslint-plugin-react": "catalog:",
    "eslint-plugin-react-hooks": "catalog:",
    "eslint-plugin-json-schema-validator": "catalog:",
    "eslint-plugin-storybook": "^10.1.11",
    "globals": "catalog:",
    "jsonc-eslint-parser": "catalog:",
    "typescript-eslint": "catalog:",
    "fast-glob": "^3.3.3",
    "jest": "catalog:",
    "jest-environment-jsdom": "catalog:",
    "prettier-plugin-sort-json": "catalog:",
    "relay-test-utils": "catalog:",
    "storybook": "^10.1.11",
    "ts-jest": "catalog:",
    "typescript": "^5.9.3",
    "vite": "^6.4.1",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-relay-lite": "^0.11.0",
    "vite-plugin-svgr": "^4.5.0"
  },
  "type": "module",
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "extensionsToTreatAsEsm": [
      ".ts",
      ".tsx"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": [
        "ts-jest",
        {
          "useESM": true,
          "tsconfig": {
            "jsx": "react-jsx"
          }
        }
      ],
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "setupFilesAfterEnv": [
      "./setupTests.ts"
    ],
    "testEnvironment": "jsdom"
  }
}
