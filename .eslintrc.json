{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "google",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "comma-dangle": 0,
    "require-jsdoc": 0,
    "max-len": 0,
    "new-cap": 0,
    "indent": ["error", 2],
    "camelcase": "off",
    "no-empty-function": "off"
  }
}
